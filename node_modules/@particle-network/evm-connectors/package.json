{
  "name": "@particle-network/evm-connectors",
  "version": "1.0.8",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "license": "Apache-2.0",
  "homepage": "https://particle.network",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "author": "Particle Network",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ethereum",
    "wallet",
    "blockchain",
    "auth",
    "mpc",
    "particle",
    "evm-connector"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "mkdir -p dist/cjs && echo '{ \"type\": \"commonjs\" }' > dist/cjs/package.json && node build.js",
    "build:dev": "NODE_ENV=development yarn build",
    "build:watch": "yarn build --watch",
    "clean": "rm -rf dist && rm -rf esm",
    "dev": "yarn clean && NODE_ENV=development yarn build:watch & NODE_ENV=development yarn typegen:watch",
    "prepare": "MINIFY_CSS=true yarn build",
    "prebuild": "yarn typegen",
    "typecheck": "yarn tsc --noEmit",
    "typegen": "tsc --emitDeclarationOnly || true",
    "typegen:watch": "tsc --emitDeclarationOnly --watch"
  },
  "dependencies": {
    "@coinbase/wallet-sdk": "4.0.3",
    "@particle-network/connector-core": "^1.0.1",
    "@peculiar/asn1-ecc": "^2.3.13",
    "@peculiar/asn1-schema": "^2.3.13",
    "@simplewebauthn/browser": "^10.0.0",
    "@walletconnect/ethereum-provider": "2.13.0",
    "@walletconnect/modal": "2.6.2",
    "mipd": "0.0.7"
  },
  "devDependencies": {
    "@simplewebauthn/typescript-types": "^8.3.4",
    "@types/crypto-js": "^4.2.2",
    "pino-pretty": "^10.3.1"
  },
  "peerDependencies": {
    "typescript": ">=5.0.4"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "gitHead": "94e3584aec30dd402c37aa40419a15ddaaca8e8e"
}
